# Vercel Environment Variables for MoodTracker
# Copy these to your Vercel dashboard under Settings > Environment Variables

# Database Configuration
POSTGRES_HOST=your_postgres_host
POSTGRES_PORT=5432
POSTGRES_DB=moodtracker
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_SSL=true

# Encryption Key for Local Storage (32 characters)
INDEXEDDB_ENCRYPTION_KEY=your_32_char_encryption_key_here

# Next.js Public Variables
NEXT_PUBLIC_API_BASE_URL=/api
NEXT_PUBLIC_WS_URL=wss://your-domain.vercel.app/ws
