{
  "name": "appointment-scheduler",
  "version": "1.0.0",
  "description": "Professional appointment scheduling system with calendar integration",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run server:dev\"",
    "build": "webpack --config webpack.prod.js",
    "build:watch": "webpack --config webpack.dev.js --watch",
    "server:dev": "nodemon src/app.js",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "db:test": "node scripts/test-connection.js",
    "deploy:vercel": "bash scripts/deploy-vercel.sh"
  },
  "keywords": [
    "appointment",
    "scheduling",
    "calendar",
    "booking",
    "nodejs",
    "react",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.6",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "tailwindcss": "^3.3.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}