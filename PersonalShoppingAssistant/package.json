{
  "name": "personal-shopping-assistant",
  "version": "1.0.0",
  "description": "A virtual personal shopping assistant that suggests products based on user preferences",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "test": "ts-node -r tsconfig-paths/register scripts/run-tests.ts",
    "test:unit": "ts-node -r tsconfig-paths/register scripts/run-tests.ts --unit",
    "test:integration": "ts-node -r tsconfig-paths/register scripts/run-tests.ts --integration",
    "test:contract": "ts-node -r tsconfig-paths/register scripts/run-tests.ts --contract",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "jest tests/integration/performance.test.ts",
    "test:security": "jest tests/integration/security.test.ts",
    "perf:benchmark": "node tests/performance/benchmark.js",
    "perf:benchmark:limited": "node tests/performance/benchmark-rate-limited.js",
    "perf:load": "node tests/performance/load-test.js",
    "perf:monitor": "node tests/performance/monitor.js",
    "perf:analyze": "node tests/performance/optimize.js",
    "perf:all": "npm run perf:benchmark:limited && npm run perf:analyze",
    "security:test": "node tests/security/security-test.js",
    "security:sql": "node tests/security/sql-injection-test.js",
    "security:headers": "node tests/security/headers-test.js",
    "security:all": "node tests/security/security-runner.js",
    "vercel-build": "node scripts/vercel-build.js",
    "vercel-dev": "vercel dev",
    "vercel-deploy": "vercel --prod",
    "vercel-logs": "vercel logs",
    "health-check": "node scripts/health-check.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register src/database/migrate.ts migrate",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts seed",
    "db:reset": "ts-node -r tsconfig-paths/register src/database/migrate.ts reset && ts-node -r tsconfig-paths/register src/database/seed.ts seed",
    "db:status": "ts-node -r tsconfig-paths/register src/database/migrate.ts status",
    "db:create:test": "PGPASSWORD=postgres createdb -U postgres personal_shopping_assistant_test",
    "db:drop:test": "PGPASSWORD=postgres dropdb -U postgres personal_shopping_assistant_test",
    "db:migrate:test": "PGPASSWORD=postgres DATABASE_URL=postgresql://postgres:postgres@localhost:5432/personal_shopping_assistant_test ts-node src/database/migrate.ts migrate",
    "db:seed:test": "PGPASSWORD=postgres DATABASE_URL=postgresql://postgres:postgres@localhost:5432/personal_shopping_assistant_test ts-node src/database/seed.ts seed",
    "dev:backend": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:frontend": "cd src/frontend && npm start",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "keywords": [
    "shopping",
    "recommendations",
    "personal-assistant",
    "react",
    "typescript",
    "nodejs"
  ],
  "author": "Personal Shopping Assistant Team",
  "license": "MIT",
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "@types/swagger-ui-express": "^4.1.8",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "styled-components": "^6.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
